{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": 0,
    "no-extra-semi": 0,
    "comma-dangle": 0,
    "indent": ["error", 2],
    "keyword-spacing": ["error", { "before": true }],
    "no-trailing-spaces": 0
  }
}
